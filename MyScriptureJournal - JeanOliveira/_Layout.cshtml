<!DOCTYPE html>
<html>
<head>
    <title>@Page.Title</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="~/Style/normalize.css" type="text/css" rel="stylesheet" media="screen">
    <link href="~/Styles/Scriptures.css" rel="stylesheet" type="text/css" media="screen" />
   
</head>
<body>
    <div id="container">
        <div id="header">
            <h1>My Scripture Journal Site</h1>
        </div>
        <div id="main">
            @RenderBody()
        </div>
        <div id="footer">
            &copy; @DateTime.Now.Year My Scripture Journal Site
        </div>
    </div>
</body>
</html>